//190. Reverse Bits in java...

class Solution {
    public int reverseBits(int n) {
        int result = 0; 
        for(int i = 0; i<32; i++){
            result <<= 1; // left shift
            result |= (n & 1); // getting LSB 
            n >>= 1; // right shift
        }
        return result;
    }
}
