//503. Next Greater Element II in java..

class Solution {
    public int[] nextGreaterElements(int[] nums) {
        int n = nums.length;
        int[] arr = new int[n];
        Stack<Integer> st = new Stack<>();
        for(int i = 2*n-1 ; i>=0 ; i--){
           
           int ind = i%n;
            while(!st.isEmpty() && st.peek()<=nums[ind]){
                st.pop();
            }

            if(i<n){
                arr[i] = st.isEmpty() ? -1 : st.peek();
            }
            st.push(nums[ind]);


        }
        return arr;
    }
}
