//205. Isomorphic Strings in java...




class Solution {
    public boolean isIsomorphic(String s, String t) {

        if(s.length() != t.length()){
            return false;
        }
        int[] mps = new int[256];
        int[] mpt = new int[256];

        for(int i = 0; i<s.length(); i++){
            if(mps[s.charAt(i)] != mpt[t.charAt(i)]){
                return false;
            }

            mps[s.charAt(i)] = i +1;
            mpt[t.charAt(i)] = i+1;
        }

        return true;

        
    }
}
