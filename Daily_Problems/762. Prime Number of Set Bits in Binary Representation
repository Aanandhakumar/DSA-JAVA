//762. Prime Number of Set Bits in Binary Representation in java...




class Solution {
    public int countPrimeSetBits(int left, int right) {
        int count = 0;
        for(int i = left; i<=right; i++){
            
            int bitsCount = Integer.bitCount(i);
            if(isPrime(bitsCount)){
                count++;
            }
        }
        return count;
       
    }

    public  static boolean isPrime(int n){
        if(n<=1){
            return false;
        }
       for(int j = 2; j<=Math.sqrt(n); j++){
        if(n%j == 0){
            return false;
        }
       
       }
        return true;

    }
}
