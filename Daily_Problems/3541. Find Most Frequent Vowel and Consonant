//3541. Find Most Frequent Vowel and Consonant in java...


class Solution {
    public int maxFreqSum(String s) {
        Set<Character> vowels = new HashSet<>(Arrays.asList('a','e','i','o','u'));
        Map<Character,Integer> mp1 = new HashMap<>();
        Map<Character,Integer> mp2 = new HashMap<>();
        int vowelsCount = 0;
        int consonentCount = 0;
        for(char c : s.toCharArray()){
            if(vowels.contains(c)){
                mp1.put(c,mp1.getOrDefault(c,0)+1);
                vowelsCount = Math.max(vowelsCount,mp1.get(c));
            }else {
                mp2.put(c,mp2.getOrDefault(c,0)+1);
                consonentCount = Math.max(consonentCount,mp2.get(c));
            }
        }

        return  consonentCount+vowelsCount;

    }
}
