//1288. Remove Covered Intervals in java...


class Solution {
    public int removeCoveredIntervals(int[][] intervals) {
        int n = intervals.length;
        Arrays.sort(intervals, (a,b) -> a[0] == b[0] ? Integer.compare(b[1],a[1]): Integer.compare(a[0],b[0]));
        
       int  count  = 0;
       int prevEnd = 0;
        for(int i = 0; i<n; i++){
            //if next end greater than previou end not coverd 
            if(intervals[i][1] > prevEnd){
                count++;
                prevEnd = intervals[i][1];
            }
            //else we skip the coverd intervals


           
        }
        return count;
        
    }
}
